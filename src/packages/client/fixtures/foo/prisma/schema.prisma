generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["groupBy"]
}

datasource db {
  provider = "postgresql"
  url      = env("PGURL")
}

model Foobar {
  id        String     @id
  buzzbar   BuzzBar    @relation(fields: [buzzbarId], references: [id])
  buzzbarId String
  name      String
  type      FoobarType
  zoozam    Json?
  baz       Foobar?    @relation(fields: [bazId], references: [id])
  bazId     String?
  buzzType  BuzzType
  Foobar    Foobar[]   @relation("FoobarToFoobar")
}

model Zoobar {
  id        String  @id
  buzzbar   BuzzBar @relation(fields: [buzzbarId], references: [id])
  buzzbarId String
  a         String
  b         String?
  c         Json?
}

model BuzzBar {
  id     String   @id
  a      Int
  b      String
  Foobar Foobar[]
  Zoobar Zoobar[]
}

enum FoobarType {
  X
  Y
}

enum BuzzType {
  A
  B
}
